<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Chequeo rapido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="88dp"
        android:text="Selecciona sintomas que presentas:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/tbFiebre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="ToggleButton"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.734"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:textOff="No presento"
        android:textOn="Si presento"
        />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Fiebre mayor a 38Â°"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.655"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Dolor de garganta"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ToggleButton
        android:id="@+id/tbDolorGarganta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="tbDolorGarganta"
        app:layout_constraintStart_toStartOf="@id/tbFiebre"
        app:layout_constraintTop_toBottomOf="@+id/tbFiebre"
        android:textOff="No presento"
        android:textOn="Si presento"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Tos"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Dificultad para respirar"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Perdida de gusto y/o olfato"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Fatiga"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Congestion nasal"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ToggleButton
        android:id="@+id/tbTos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:text="tbTos"
        app:layout_constraintBottom_toTopOf="@+id/tbDificultadRespirar"
        app:layout_constraintStart_toStartOf="@+id/tbDolorGarganta"
        app:layout_constraintTop_toBottomOf="@+id/tbDolorGarganta"
        android:textOff="No presento"
        android:textOn="Si presento"/>

    <ToggleButton
        android:id="@+id/tbDificultadRespirar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="tbDificultadRespirar"
        app:layout_constraintStart_toStartOf="@+id/tbTos"
        app:layout_constraintTop_toBottomOf="@+id/tbTos"
        android:textOff="No presento"
        android:textOn="Si presento"/>

    <ToggleButton
        android:id="@+id/tbPerdidaGustoOlfato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="tbPerdidaGustoOlfato"
        app:layout_constraintStart_toStartOf="@+id/tbDificultadRespirar"
        app:layout_constraintTop_toBottomOf="@+id/tbDificultadRespirar"
        android:textOff="No presento"
        android:textOn="Si presento"/>

    <ToggleButton
        android:id="@+id/tbFatiga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:text="tbFatiga"
        app:layout_constraintBottom_toTopOf="@+id/tbCongestionNasal"
        app:layout_constraintStart_toStartOf="@+id/tbPerdidaGustoOlfato"
        app:layout_constraintTop_toBottomOf="@+id/tbPerdidaGustoOlfato"
        android:textOff="No presento"
        android:textOn="Si presento"/>

    <ToggleButton
        android:id="@+id/tbCongestionNasal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="tbCongestionNasal"
        app:layout_constraintStart_toStartOf="@+id/tbFatiga"
        app:layout_constraintTop_toBottomOf="@+id/tbFatiga"
        android:textOff="No presento"
        android:textOn="Si presento"/>

    <Button
        android:id="@+id/btn_chequear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="chequear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>